@{
    ViewBag.Ref = "HandBook";
    ViewBag.Title = "格蛇图鉴";
}

    <div class="text-center">
        <h1>格蛇志</h1>
        <div>
            @for (int i = 0; i < Model.snakes.Count; i++)
            {
                <div class="row">
                    <div class="col-md-8">
                        <a class="navbar-brand" asp-controller="HandBook" asp-action="Snake" asp-route-SnakeName="@Model.snakes[i]">@Model.snakes[i]</a>
                    </div>
                    <div class="col-md-4">No.@i</div>
                </div>
            }
        </div>
    </div>
